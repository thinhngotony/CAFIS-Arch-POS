    <Grid>
        <Grid x:Name="axOpGrid" Visibility="Hidden"/>
        <Grid Height="768" Width="1024" >
            <Popup x:Name="PopupNotification" IsOpen="False" Height="638" Width="860" PlacementRectangle="82,65,860,0" PlacementTarget="{Binding ElementName=MainWindowUI}">
                <Grid Height="638" Width="860" Background="White">
                    <Label VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Meiryo" FontSize="44" Margin="0,0,0,80">
                        <TextBlock Name="MessageNotification" Text="12341234" Foreground="{Binding BodyTextColor}" TextAlignment="Center"
                            LineHeight="70" TextWrapping="Wrap" LineStackingStrategy="BlockLineHeight" />
                    </Label>
                    <Button
                        x:Name="PopupBtnOK"
                        VerticalAlignment="Bottom" HorizontalAlignment="Center"
                        Margin="0,0,0,13" Width="410" Height="105"
                        FontFamily="MS Gothic" Style="{StaticResource NormalButtonStyle}"
                        Content="OK" FontSize="44" Click="PopupBtnOKClick">
                    </Button>
                </Grid>
            </Popup>
            <ContentControl x:Name="ContentControl" Content="{Binding Path=CurrentViewModel}" Margin="0,0,0,0"/>
        </Grid>
    </Grid>
